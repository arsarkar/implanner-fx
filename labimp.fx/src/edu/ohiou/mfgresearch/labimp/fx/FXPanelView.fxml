<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import edu.ohiou.mfgresearch.labimp.fx.DrawFXCanvas?>
<?import javafx.scene.layout.BorderPane?>

<fx:root prefHeight="800.0" prefWidth="1000.0" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <TableView fx:id="targetView" prefHeight="600.0" prefWidth="311.0" BorderPane.alignment="CENTER">
        <columns>
        <TableColumn fx:id="targetCol" maxWidth="5000.0" minWidth="10.0" prefWidth="75.0" text="Target" />
        <TableColumn fx:id="showCol" maxWidth="5000.0" minWidth="10.0" prefWidth="75.0" text="Show" />
        <TableColumn fx:id="hideCol" maxWidth="5000.0" minWidth="10.0" prefWidth="75.0" text="Hide" />
        <TableColumn fx:id="setActiveCol" prefWidth="75.0" text="Set Active" />
        </columns>
        <columnResizePolicy>
		<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
		</columnResizePolicy>
      </TableView>
   </left>
   <center>
      <SplitPane dividerPositions="0.70" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0" BorderPane.alignment="CENTER">
         <items>
            <DrawFXCanvas fx:id="canvas" />
            <Pane prefHeight="200.0" prefWidth="200.0" />
         </items>
      </SplitPane>
   </center>
</fx:root>
